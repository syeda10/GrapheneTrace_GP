@model List<GrapheneTrace_GP.Areas.Admin.ViewModels.ClinicianListVM>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>S.no</th>
            <th>Title</th>
            <th>Last Name</th>
            <th>First Name</th>
            <th>Clinician Id</th>
            <th>Speciality</th>
            <th>Age</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <td>@(i + 1)</td>
                <td>@Model[i].Title</td>
                <td>@Model[i].ClinicianLastName</td>
                <td>@Model[i].ClinicianFirstName</td>
                <td>@Model[i].ClinicianId</td>
                <td>@Model[i].Speciality</td>
                <td>@Model[i].ClinicianAge</td>
            </tr>
        }
    </tbody>
</table>
